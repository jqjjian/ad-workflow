# 应用配置
APP_IMAGE=ad-workflow-x86:latest
APP_CONTAINER_NAME=ad-workflow-app-x86
APP_PORT=3000
NODE_ENV=production

# NextAuth配置 - 关键配置，必须一致
NEXTAUTH_URL=https://myad.shopwis.cn
# 容器内部使用的URL - 关键变量，解决反向代理问题
NEXTAUTH_URL_INTERNAL=http://localhost:3000
NEXTAUTH_SECRET=J1jhQbNzHQ5VUtfoErPejEdDpL0z7rAYSCPfvBpQslk=
AUTH_SECRET=J1jhQbNzHQ5VUtfoErPejEdDpL0z7rAYSCPfvBpQslk=

# Cookie域名配置 - 必须与访问域名一致
NEXTAUTH_COOKIE_DOMAIN=myad.shopwis.cn

# 信任主机配置
NEXTAUTH_TRUSTED_HOSTS=myad.shopwis.cn,localhost,127.0.0.1
NEXTAUTH_TRUST_HOST=true

# 公共URL - 客户端访问用
NEXT_PUBLIC_APP_URL=https://myad.shopwis.cn

# Nginx反向代理支持
PROXY_PROTOCOL=true
# Server Actions安全配置
NEXT_PUBLIC_SERVER_ACTIONS_ALLOWED_ORIGIN=myad.shopwis.cn

# API配置
ACCESS_TOKEN_SECRET=ad776656d49f4adb840ef6187115fb8b
